{
	"name": "supabase",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"@hookform/resolvers": "^2.8.8",
		"@reduxjs/toolkit": "^1.8.0",
		"@supabase/supabase-js": "^1.31.1",
		"axios": "^0.24.0",
		"classnames": "^2.3.1",
		"match-sorter": "^6.3.1",
