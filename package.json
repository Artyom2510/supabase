		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-hook-form": "^7.27.1",
		"react-redux": "^7.2.6",
		"react-router-dom": "^6.2.2",
		"react-scripts": "4.0.3",
		"redux": "^4.1.2",
